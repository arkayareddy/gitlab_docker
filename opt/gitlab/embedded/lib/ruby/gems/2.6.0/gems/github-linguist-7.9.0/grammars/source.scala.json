          "beginCaptures": {
            "1": {
              "name": "punctuation.definition.comment.scala"
            }
          },
          "endCaptures": {
            "0": {
              "name": "punctuation.definition.comment.scala"
            }
          }
        },
        {
          "name": "comment.block.scala",
          "begin": "/\\*",
          "end": "\\*/",
          "patterns": [
            {
              "include": "#block-comments"
            }
          ],
          "captures": {
            "0": {
              "name": "punctuation.definition.comment.scala"
            }
          }
        }
      ]
    },
    "char-literal": {
